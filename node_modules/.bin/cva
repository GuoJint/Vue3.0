#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_create-vite-app@1.18.0@create-vite-app/index.js" "$@"
  ret=$?
else 
  node  "$basedir/../_create-vite-app@1.18.0@create-vite-app/index.js" "$@"
  ret=$?
fi
exit $ret
